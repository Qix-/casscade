{
	"name": "@qix/casscade",
	"private": true,
	"version": "0.0.0",
	"license": "PROPRIETARY",
	"scripts": {
		"format": "prettier --write .",
		"format:staged": "pretty-quick --staged",
		"build": "gulp"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "11.2.1",
		"@vercel/git-hooks": "1.0.0",
		"gulp": "4.0.2",
		"gulp-rollup-2": "1.3.1",
		"kiwi.js": "1.1.2",
		"prettier": "2.2.1",
		"pretty-quick": "3.1.0",
		"rollup": "2.45.2",
		"rollup-plugin-pegjs": "2.1.3",
		"rollup-plugin-terser": "7.0.2"
	},
	"git": {
		"pre-commit": [
			"format:staged"
		]
	},
	"prettier": {
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "none",
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"requirePragma": false,
		"insertPragma": false,
		"endOfLine": "lf"
	}
}
